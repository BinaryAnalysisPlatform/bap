long         sysconf(int);
pid_t        tcgetpgrp(int);
int          tcsetpgrp(int, pid_t);
int          truncate(const char *file, off_t length) __attribute__((warn_unused_result));
char        *ttyname(int);
int          ttyname_r(int, char *, size_t);
int          unlink(const char *name);
int          unlinkat(int fd, const char *name, int flags);
