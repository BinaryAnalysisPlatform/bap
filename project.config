[project]
	description = Binary Analysis Platform Meta-Project\n\
\n\
This project serves to provide working sets of the various bap components via git submodules.
[receive]
	requireSignedOffBy = false
[access "refs/heads/master"]
	submit = group ldap/aegis
	submit = group ldap/penn
	label-Code-Review = -2..+2 group ldap/okahn
	label-Code-Review = -2..+2 group user/Maverick Woo (maverick)
	read = group ldap/aegis
	read = group ldap/penn
	abandon = group ldap/aegis
	abandon = group ldap/penn
[access "refs/*"]
	read = group ldap/aegis
	read = group ldap/penn
